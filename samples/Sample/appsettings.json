{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "SampleRoutes": {
        "AuthorizationPolicy": "default",
        "ClusterId": "SampleCluster",
        "Match": {
          "Path": "/account"
        },
        "Transforms": [
          { "AppendClaim": "user-id" }
        ]
      }
    },
    "Clusters": {
      "SampleCluster": {
        "Destinations": {
          "destination1": {
            "Address": "https://httpbin.org/anything/"
          }
        }
      }
    }
  },
  "AllowedHosts": "*"
}
